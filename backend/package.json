{
  "name": "videochat-mlm-backend",
  "version": "1.0.0",
  "description": "API Backend for VideoChat MLM Application with PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "npm run db:deploy && npm run start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy && npm run db:seed:prod",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts",
    "db:seed:prod": "node dist/seed.js"
  },
  "keywords": ["videochat", "mlm", "api", "backend", "postgresql"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.12.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prisma": "^5.9.0"
  }
}
